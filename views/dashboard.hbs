	<style>
			.block{
				height:500px;
			}
			#mytable tr:hover {
    			cursor: pointer;
			}
			.header {
				letter-spacing: 1px;
		 		height: 80px;
			}
			.bg{
				background-color: #f7f7f9;;
			}
			.button{
				position: absolute;
				top:1%;
				left: 1%;
				z-index: 1000;
				font-weight: bold;
				letter-spacing: 1px;
				padding:10px 20px;
				border-radius: 10px;
				outline: none;
				text-decoration: none;
				box-shadow: 0px 3px 8px #5f2bla;
			}
		</style>
