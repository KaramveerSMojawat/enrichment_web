 $(`#mytable td`).on("click", function() {
 	var currentrow=	$(this).eq(0).html();
 	var data = $(this).first(0).text();
 	var data1	= $(this).find('td:first').text();
	var id = $(this).first(0).attr("id");   	
   	if(id !== undefined){
   		window.location = "/articles/" + data;	
   	}
});
 $(`#mytable td`).hover(function() {
 	var currentrow=	$(this).eq(0).html();
 	var data = $(this).first(0).text();
 	var data1	= $(this).find('td:first').text();
	var id = $(this).first(0).attr("id");   	
   	if(id !== undefined){
   		$('#'+id).hover(function() {
        	$('.statecontent').hide();
        	$('.' + $(this).val()).show();    
    	});	
   	}
});

$('.page-item').on("click", function() {
 	var currentrow =	$(this).eq(0).html();
 	var data = $(this).first(0).text();
   	window.location = "/dashboard/data/" + data;
});
</script>
